{
  "name": "prepostagem_magnojet",
  "version": "1.0.0",
  "description": "Plataforma de Pré-Postagem da MagnoJet com integração via API 36 do CWS",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "concurrently -n vite,laravel -k \"vite\" \"php artisan serve --host magnojet.prepostagem --port 8000\"",
    "lint:check": "prettier --check .",
    "lint:fix": "prettier --write .",
    "test": "php artisan test",
    "test:watch": "nodemon --exec ./vendor/bin/phpunit --ext php --watch tests --watch src"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.12",
    "@tailwindcss/vite": "^4.1.12",
    "autoprefixer": "^10.4.21",
    "axios": "^1.8.2",
    "concurrently": "^9.0.1",
    "laravel-vite-plugin": "^1.2.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.12",
    "vite": "^6.2.4"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.1.12"
  }
}
