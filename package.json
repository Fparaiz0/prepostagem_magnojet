{
  "name": "prepostagem_magnojet",
  "version": "1.0.0",
  "description": "Plataforma de Pré-Postagem da MagnoJet com integração via API 36 do CWS",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "concurrently -n vite,laravel,schedule -k \"vite\" \"php artisan serve --host magnojet.prepostagem --port 8000\" \"php artisan schedule:work\"",
    "lint:check": "prettier --check .",
    "lint:fix": "prettier --write .",
    "test": "php artisan test",
    "test:watch": "nodemon --exec ./vendor/bin/phpunit --ext php --watch tests --watch src",
    "prepare": "husky",
    "commit": "cz"
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@tailwindcss/postcss": "4.1.12",
    "@tailwindcss/vite": "4.1.12",
    "autoprefixer": "10.4.21",
    "axios": "1.8.2",
    "commitizen": "4.3.0",
    "concurrently": "9.0.1",
    "cz-conventional-changelog": "3.3.0",
    "dotenv": "17.2.3",
    "husky": "9.1.4",
    "laravel-vite-plugin": "1.2.0",
    "nodemon": "3.1.10",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "tailwindcss": "4.1.12",
    "vite": "6.2.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "dependencies": {
      "@tailwindcss/cli": "4.1.12"
    }
  }
}
